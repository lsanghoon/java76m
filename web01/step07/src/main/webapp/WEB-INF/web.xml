	version="3.1" metadata-complete="true">

	<!-- 서블릿 클래스에 대해 선언한다. -->
		<!-- 서블릿 별명을 선언한다. 공백을 포함할 수 있다. 마음대로 작성하라. 그러나 프로그래밍의 일관성을 위해 클래스 이름으로 
			작성하는 것이 좋다. -->

	<servlet>
		<servlet-name>Servlet01</servlet-name>
		<servlet-class>servlet.Servlet01</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet02</servlet-name>
		<servlet-class>servlet.Servlet02</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet03Step1</servlet-name>
		<servlet-class>servlet.Servlet03Step1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet03Step2</servlet-name>
		<servlet-class>servlet.Servlet03Step2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet03Step3</servlet-name>
		<servlet-class>servlet.Servlet03Step3</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Servlet04Step1</servlet-name>
		<servlet-class>servlet.Servlet04Step1</servlet-class>
	</servlet>
	<servlet>
    <servlet-name>Servlet04Step2</servlet-name>
    <servlet-class>servlet.Servlet04Step2</servlet-class>
  </servlet>
  <servlet>
