  </servlet>
  <servlet>
    <servlet-name>Servlet06Step2</servlet-name>
    <servlet-class>servlet.Servlet06Step2</servlet-class>
  </servlet>
	
	
	<servlet>
    <servlet-name>OtherServlet</servlet-name>
    <servlet-class>servlet.OtherServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet02Sum</servlet-name>
    <servlet-class>servlet.Servlet02Sum</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet02Aver</servlet-name>
    <servlet-class>servlet.Servlet02Aver</servlet-class>
  </servlet>
  
  
  
  

	<!-- 서블릿 클래스에 URL을 부여한다. -->
		<!-- URL 이름을 작성할 때 공백을 주어서는 안된다. 확장자명은 마음대로. 사용자가 웹브라우저에서 요청할 때는 다음과 같이 
			요청해야 한다. http://localhost:8080/web01/exam/servlet01 -->
	<servlet-mapping>
		<servlet-name>Servlet01</servlet-name>
		<url-pattern>/step07/servlet01</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet02</servlet-name>
		<url-pattern>/step07/servlet02</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Servlet03Step1</servlet-name>
		<url-pattern>/step07/servlet03step1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
